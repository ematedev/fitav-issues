<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>2</portlet-name>
		<display-name>Esf Organization</display-name>
		<portlet-class>it.ethica.esf.portlet.ESFOrganizationPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/organizationadmin/view.jsp</value>
		</init-param>
		<init-param>
			<name>template-path</name>
			<value>/organizationadmin/</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Esf Organization</title>
			<short-title>Esf Organization</short-title>
			<keywords>Esf Organization</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>3</portlet-name>
		<display-name>Esf Field</display-name>
		<portlet-class>
			it.ethica.esf.portlet.ESFFieldPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/fieldadmin/view.jsp</value>
		</init-param>
		<init-param>
			<name>template-path</name>
			<value>/fieldadmin/</value>
		</init-param>
		
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Esf Field</title>
			<short-title>Esf Field</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>4</portlet-name>
		<display-name>Esf Sport Type</display-name>
		<portlet-class>
			it.ethica.esf.portlet.ESFSportTypePortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/sporttypeadmin/view.jsp</value>
		</init-param>
		<init-param>
			<name>template-path</name>
			<value>/sporttypeadmin/</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Esf Sport Type</title>
			<short-title>5</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>